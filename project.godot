; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Context",
"language": "GDScript",
"path": "res://src/resource_classes/context.gd"
}, {
"base": "Resource",
"class": "DropItem",
"language": "GDScript",
"path": "res://src/resource_classes/drop_item.gd"
}, {
"base": "Resource",
"class": "EnemyData",
"language": "GDScript",
"path": "res://src/resource_classes/enemy_data.gd"
}, {
"base": "Button",
"class": "ItemButton",
"language": "GDScript",
"path": "res://src/game/item_button.gd"
}, {
"base": "Resource",
"class": "ItemPool",
"language": "GDScript",
"path": "res://src/resource_classes/item_pool.gd"
}, {
"base": "Sprite",
"class": "PlayerBase",
"language": "GDScript",
"path": "res://src/game/battle/player_base.gd"
}, {
"base": "Resource",
"class": "Snippet",
"language": "GDScript",
"path": "res://addons/light_snippet/snippet.gd"
}, {
"base": "Resource",
"class": "Weapon",
"language": "GDScript",
"path": "res://src/resource_classes/weapon.gd"
} ]
_global_script_class_icons={
"Context": "",
"DropItem": "",
"EnemyData": "",
"ItemButton": "",
"ItemPool": "",
"PlayerBase": "",
"Snippet": "",
"Weapon": ""
}

[application]

config/name="Fragile Blacksmith"
run/main_scene="res://src/scene_manager.tscn"
config/icon="res://icon.png"

[autoload]

CONFIG="*res://src/singletons/config.gd"
LOG="*res://src/singletons/log.gd"
UTILS="*res://src/singletons/utils.gd"
TWEEN="*res://src/singletons/tween_manager.tscn"
WEAPON_FACTORY="*res://src/singletons/weapon_factory.gd"

[debug]

gdscript/warnings/treat_warnings_as_errors=true
gdscript/warnings/unsafe_property_access=true

[display]

window/size/width=1536
window/size/height=900
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/light_snippet/plugin.cfg" )

[layer_names]

2d_physics/layer_1="world"
2d_physics/layer_3="player"
2d_physics/layer_6="enemy"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
